		<form id="registrationForm" action="${pageContext.request.contextPath}/signup" method="post"
			class="signin">
			<div class="content">
				<h2>Registration</h2>
				<div class="form">
					<div class="inputBox">
						<input type="text" id="username" name="username" required>
						<i>Username</i> <span id="usernameError" class="error"></span>
					</div>
					<div class="inputBox">
						<input type="email" id="email" name="email" required> <i>Email</i>
						<span id="emailError" class="error"></span>
					</div>
					<div class="inputBox">
						<input type="password" id="password" name="password" required>
						<i>Password</i> <span id="passwordError" class="error"></span>
					</div>
					<div class="inputBox">
						<input type="password" id="confirmPassword" name="confirmPassword"
							required> <i>Confirm Password</i> <span
							id="confirmPasswordError" class="error"></span>
					</div>
					<div class="links">
						<a href="#">Already have an Account?</a> <a href="${pageContext.request.contextPath}/login">Login</a>
					</div>
					<div class="inputBox">
						<input type="submit" value="Register">
					</div>
				</div>
			</div>
		</form>
